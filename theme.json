{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "1240px",
			"wideSize": "1400px"
		},
		"color": {
			"defaultPalette": false,
			"custom": true,
			"palette": [
				{
					"color": "#182156",
					"name": "Blue",
					"slug": "blue-color"
				},
				{
					"color": "#1D4A90",
					"name": "Light Blue",
					"slug": "light-blue-color"
				},
				{
					"color": "#B2D7F1",
					"name": "Sky Blue",
					"slug": "sky-blue-color"
				},
				{
					"color": "#F05124",
					"name": "Orange",
					"slug": "orange-color"
				},
				{
					"color": "#033",
					"name": "Green",
					"slug": "green-color"
				},
				{
					"color": "#9CCB48",
					"name": "Light Green",
					"slug": "light-green-color"
				},
				{
					"color": "#EBF5DA",
					"name": "Lemon Green",
					"slug": "lemon-green-color"
				},
				{
					"color": "#FFB3A2",
					"name": "Pink",
					"slug": "pink-color"
				},
				{
					"color": "#F58634",
					"name": "Yellow",
					"slug": "yellow-color"
				},
				{
					"color": "#f8f8f8",
					"name": "White Shade",
					"slug": "white-shade"
				},
				{
					"color": "#1E1E1E",
					"name": "Black Shade",
					"slug": "black-shade"
				}
			]
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Nunito",
					"slug": "nunito",
					"fontFace": [
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/nunito_normal_200.ttf"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/nunito_normal_300.ttf"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/nunito_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/nunito_normal_500.ttf"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/nunito_normal_600.ttf"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/nunito_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/nunito_normal_800.ttf"
							]
						},
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/nunito_normal_900.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid":false,
					"name": "Small",
					"size": "12px",
					"slug": "small"
				},
				{
					"fluid":false,
					"name": "Medium",
					"size": "16px",
					"slug": "medium"
				},
				{
					"fluid": false,
					"name": "Large",
					"size": "20px",
					"slug": "large"
				},
				{
					"fluid": false,
					"name": "Extra Large",
					"size": "48px",
					"slug": "extra-large"
				},
				{
					"fluid":false,
					"name": "Extra Extra Large",
					"size": "60px",
					"slug": "extra-extra-large"
				}
			]
		},
		"shadow": {
			"presets": [
				{
					"name": "Natural",
					"slug": "natural",
					"shadow": "6px 6px 9px rgba(0, 0, 0, 0.2)"
				},
				{
					"name": "Crisp",
					"slug": "crisp",
					"shadow": "6px 6px 0px rgba(0, 0, 0, 1)"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"elements": {
			"h1": {
				"typography": {
					"fontSize":""
				}
			},
			"link": {
				":hover": {
					"typography": {
						"textDecoration":"none"
					}
				},
				"typography": {
					"textDecoration":"none"
				}
			}
		},
		"typography": {
			"fontFamily":"var(--wp--preset--font-family--nunito)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "blog",
			"postTypes": [
				"page"
			],
			"title": "Blog"
		},
		{
			"name": "sidebar",
			"postTypes": [
				"post"
			],
			"title": "Sidebar"
		}
	],
	"version": 2
}